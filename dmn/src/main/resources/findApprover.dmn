<?xml version="1.0" encoding="UTF-8"?>
<Definitions xmlns="http://www.omg.org/spec/DMN/20130901" id="definitions" name="camunda" 
namespace="http://camunda.org/dmn"
xmlns:camunda="http://camunda.org/schema/1.0/dmn">
    <Decision id="decision" name="CheckOrder">
        <DecisionTable id="decisionTable" name="FindApprover" isComplete="true" isConsistent="true">
            <clause id="clause1" name="Customer Status">
                <inputExpression id="inputExpression1" name="Status">
                    <text>sheet.customerStatus</text>
                </inputExpression>
                <inputEntry id="inputEntry1">
                    <text>#{sheet.customerStatus == 'BRONZE'}</text>
                </inputEntry>
                <inputEntry id="inputEntry2">
                    <text>#{sheet.customerStatus == 'SILVER'}</text>
                </inputEntry>
                <inputEntry id="inputEntry3">
                    <text>#{sheet.customerStatus == 'GOLD'}</text>
                </inputEntry>
            </clause>
            <clause id="clause2" name="Approval Sum">
                <inputExpression id="inputExpression2" name="Sum">
                    <text>sheet.approvalSum</text>
                </inputExpression>
                <inputEntry id="inputEntry4">
                    <text><![CDATA[#{sheet.approvalSum < 1000}]]></text>
                </inputEntry>
                <inputEntry id="inputEntry5">
                    <text><![CDATA[#{sheet.approvalSum >= 1000}]]></text>
                </inputEntry>
            </clause>
            <clause id="clause3" name="Approver" camunda:output="group">
                <outputEntry id="outputEntry1">
                    <text>customerService</text>
                </outputEntry>
                <outputEntry id="outputEntry2">
                    <text>premiumCustomerService</text>
                </outputEntry>
                <outputEntry id="outputEntry3">
                    <text>vipCustomerService</text>
                </outputEntry>
            </clause>
            <rule id="rule1">
                <condition>inputEntry1</condition>
                <conclusion>outputEntry1</conclusion>
            </rule>
            <rule id="rule2">
                <condition>inputEntry2</condition>
                <condition>inputEntry4</condition>
                <conclusion>outputEntry2</conclusion>
            </rule>
            <rule id="rule3">
                <condition>inputEntry2</condition>
                <condition>inputEntry5</condition>
                <conclusion>outputEntry3</conclusion>
            </rule>
            <rule id="rule4">
                <condition>inputEntry3</condition>
                <conclusion>outputEntry3</conclusion>
            </rule>
        </DecisionTable>
    </Decision>
</Definitions>
